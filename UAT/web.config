<configuration>  
<system.webServer>
<handlers>
<add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\UAT\myvenv\Scripts\python.exe|D:\UAT\myvenv\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
</handlers>
</system.webServer>
<appSettings>
<add key="WSGI_HANDLER" value="router.dash_app" /> <!-- {name_of_file}.{name_of_flask_app}-->
<add key="PYTHONPATH" value="D:\UAT" /><!-- path to venv-->
<add key="WSGI_LOG" value="D:\UAT\app.log" /> <!-- path to log-->
</appSettings>
</configuration>  